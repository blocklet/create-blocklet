{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext .mjs,.js,.jsx",
    "serve": "vite preview",
    "clean": "rm -rf .blocklet",
    "bundle": "npm run clean && vite build --base /.blocklet/proxy/<%= did %>/ && blocklet bundle --zip --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "prepare": "husky install"
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-solid": "0.4.7",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "vite": "^2.9.6",
    "vite-plugin-solid": "^2.2.6",
    "vite-plugin-blocklet": "^0.1.2",
    "vite-plugin-html": "^3.2.0"
  },
  "dependencies": {
    "@arcblock/did-auth": "^1.16.4",
    "@arcblock/did-auth-storage-nedb": "^1.6.3",
    "@blocklet/sdk": "^1.7.9",
    "@ocap/client": "^1.16.4",
    "@ocap/mcrypto": "^1.16.4",
    "@ocap/wallet": "^1.16.4",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.0",
    "express-async-errors": "^3.1.1",
    "express-history-api-fallback": "^2.2.1",
    "solid-js": "^1.3.17",
    "vite-plugin-html": "^3.2.0"
  },
  "lint-staged": {
    "*.{mjs,js,vue}": [
      "prettier --write",
      "eslint --ext .js,.vue,.mjs"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  }
}
