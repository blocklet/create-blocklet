{
  "name": "page-site",
  "private": true,
  "scripts": {
    "dev": "xmark start",
    "start": "npm run dev",
    "build": "xmark build",
    "clean": "rm -rf .blocklet",
    "bundle": "npm run clean && npm run build -- --base=/.blocklet/proxy/z8iZrWdW8s5t9p6ndZLW8PmHcBtLdxGVyGN3L/ && blocklet bundle --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json"
  },
  "dependencies": {
    "@xmark/cli": "^2.0.51",
    "@xmark/theme-arcblock": "^2.0.51",
    "react": "^18.2.0"
  }
}
