{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "dev": "blocklet dev",
    "start": "vite",
    "lint": "eslint src --ext .mjs,.js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "serve": "vite preview",
    "clean": "node scripts/build-clean.js",
    "bundle": "npm run clean && vite build && blocklet bundle --zip --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "deploy:child": "npm run bundle && blocklet deploy .blocklet/bundle --app-did=<%= did %>",
    "dev:child": "blocklet dev --component --app-did=<%= did %>",
    "prepare": "husky install",
    "bump-version": "zx scripts/bump-version.mjs"
  },
  "license": "MIT",
  "devDependencies": {
    "bumpp": "^8.2.1",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-solid": "0.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^12.5.0",
    "prettier": "^2.8.4",
    "rimraf": "^3.0.2",
    "vite": "^3.2.5",
    "vite-plugin-blocklet": "^0.5.17",
    "vite-plugin-node-polyfills": "^0.7.0",
    "vite-plugin-solid": "^2.5.0",
    "zx": "^7.1.1"
  },
  "dependencies": {
    "solid-js": "^1.6.11"
  },
  "lint-staged": {
    "*.{mjs,js,vue}": [
      "prettier --write",
      "eslint --ext .js,.vue,.mjs"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  }
}
