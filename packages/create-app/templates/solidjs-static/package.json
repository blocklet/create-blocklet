{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "dev": "blocklet dev",
    "start": "vite",
    "lint": "eslint src --ext .mjs,.js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "serve": "vite preview",
    "clean": "rm -rf .blocklet",
    "bundle": "npm run clean && vite build && blocklet bundle --zip --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "deploy:child": "npm run bundle && blocklet deploy .blocklet/bundle --app-did=<%= did %>",
    "dev:child": "blocklet dev --component --app-did=<%= did %>",
    "prepare": "husky install"
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-solid": "0.7.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.5.0",
    "prettier": "^2.7.1",
    "vite": "^3.1.4",
    "vite-plugin-blocklet": "^0.4.66",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-solid": "^2.3.9"
  },
  "dependencies": {
    "solid-js": "^1.5.7"
  },
  "lint-staged": {
    "*.{mjs,js,vue}": [
      "prettier --write",
      "eslint --ext .js,.vue,.mjs"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  }
}
