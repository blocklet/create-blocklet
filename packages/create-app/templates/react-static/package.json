{
  "name": "template-react",
  "version": "0.1.0",
  "scripts": {
    "dev": "blocklet dev",
    "eject": "vite eject",
    "serve": "vite preview",
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "start": "vite --host",
    "clean": "node scripts/build-clean.js",
    "bundle": "npm run clean && vite build && blocklet bundle --zip --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "deploy:child": "npm run bundle && blocklet deploy .blocklet/bundle --app-did=<%= did %>",
    "dev:child": "blocklet dev --component --app-did=<%= did %>",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{mjs,js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2"
  },
  "devDependencies": {
    "@arcblock/eslint-config": "^0.2.3",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^8.25.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.5.0",
    "prettier": "^2.7.1",
    "vite": "^3.1.8",
    "vite-plugin-blocklet": "^0.4.79",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svgr": "^2.2.2"
  }
}
