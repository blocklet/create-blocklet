{
  "name": "docs-site",
  "private": true,
  "scripts": {
    "dev": "blocklet dev",
    "dev:child": "blocklet dev --component --app-did=z8iZuvjcXn3Lj1DXsaKkAPvL4y1nf5SSsUsXc --mount-point=docs",
    "start": "xmark start",
    "build": "xmark build",
    "clean": "rm -rf .blocklet",
    "bundle": "npm run clean && npm run build && blocklet bundle --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "deploy:child": "npm run bundle && blocklet deploy .blocklet/bundle --app-did=z8iZuvjcXn3Lj1DXsaKkAPvL4y1nf5SSsUsXc --mount-point=docs",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json"
  },
  "dependencies": {
    "@xmark/cli": "^2.2.13",
    "@xmark/client": "^2.2.13",
    "@xmark/theme-docs": "^2.2.13"
  },
  "version": "0.4.28"
}
